    "description": "This Symfony2 bundle loads in CampaignMonitor as a service. So you can subscribe/unsubscribe members to CampaignMonitor.",
    "keywords": ["symfony2", "bundle", "Campaign Monitor", "php"],
    "homepage": "https://github.com/mailmotor/campaignmonitor-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen Desloovere",
            "email": "info@jeroendesloovere.be",
            "homepage": "http://jeroendesloovere.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4||^8.0",
        "campaignmonitor/createsend-php": "^7.0",
        "mailmotor/mailmotor-bundle": "^4.0"
    },
    "autoload": {
        "psr-4": { "MailMotor\\Bundle\\CampaignMonitorBundle\\": "" }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
